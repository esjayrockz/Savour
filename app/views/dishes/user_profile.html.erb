
<body class = "restaurant_profile">
<h3>My Reviews</h3>
  <% @reviews.each do |review| %>

  <p><%= review.comment %>&nbsp &nbsp &nbsp
    <%= link_to review.dish.dish,review.dish %>&nbsp &nbsp &nbsp
    <%= review.dish.user.name %>&nbsp &nbsp &nbsp
<small style="color:grey;"> Submitted <%= time_ago_in_words(review.created_at) %> ago </small>
</p>
<% end %>
</body>
